{
    "name": "template-vue",
    "description": "A Phaser 3 template using Vue.",
    "version": "0.0.1",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-vue.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-vue/issues"
    },
    "homepage": "https://github.com/phaserjs/template-vue#readme",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "phaser": "^3.70.0",
        "vue": "^3.4.15"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-prettier": "^8.0.0",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "prettier": "^3.0.3",
        "terser": "^5.27.0",
        "vite": "^5.0.11"
    }
}